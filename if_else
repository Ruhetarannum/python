mail='ruhetarannum@gmail.com'
pword='12345'
otp=23456
email=str(input('enter your email id:'))
passw=str(input('enter your password:'))
if(email==mail):
    if(passw==pword):
        print('login successful')
        otpp=int(input('enter otp:'))
        if(otpp==otp):
            print('entered correct otp,transaction successful')
        else:
            print('transaction failed due to incorrect otp')
    else:
        print('login unsuccessful due to incorrect password')
else:
    print('login unsuccessful due to incorrect email')
